{
    "manifest_version": 2,
    "name": "StumbleBack",
    "description": "StumbleBack takes you to a random page from your browser history. It's StumbleUpon for the past.",
    "version": "1.0",
    "permissions": [
      "history",
      "storage",
      "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "32": "icon32.png"
        }
    }
}
